{
  "name": "vscode-instance-rename",
  "displayName": "VS Code Instance Rename",
  "description": "Rename VS Code window instances for easier identification when running multiple instances",
  "version": "0.1.0",
  "publisher": "WrecklessEngineer",
  "icon": "./assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-tantan/vscode-instance-rename-extension.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "window",\
    "title",
    "rename",
    "instance",
    "workspace",
    "productivity"
  ],
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-instance-rename.rename",
        "title": "Rename VS Code Instance"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/vscode": "^1.75.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "typescript": "^5.7.3",
    "vscode-test": "^1.6.1"
  }
}
